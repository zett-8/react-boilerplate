{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["dom", "esnext"],
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "allowJs": false,
    "jsx": "react",
    "noEmit": false,
    "skipLibCheck": true,
    "esModuleInterop": true
  }
}
